#include <iostream>
using namespace std;
int main() 
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int total=(n+1)*(n+2)/2;  
    int sum=0;
    for(int i=0;i<n;i++) 
    {
        sum+=arr[i];
    }
    int missing=total-sum;
    cout<<missing<<endl;
    return 0;
}
