#include<iostream>
using namespace std;
int main()
{
    int choice;
    cin>>choice;
    switch(choice)
    {
        case 1:
        case 3:
        case 5:
        case 7:
        case 9:
        cout<<"Odd";
        break;
        
        case 0:
        case 2:
        case 4:
        case 6:
        case 8:
        case 10:
        cout<<"Even";
        break;
        
        default:
        cout<<"Invalid Input";
        
    }
}
