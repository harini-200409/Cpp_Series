#include <iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int count=0,sum=5;
    for(int i=1;i;i++)
    {
        if(i%2!=0)
        {
            sum=sum+i;
            cout<<sum<<" ";
            count++;
        }
        if(count==n)
        {
            break;
        }
    }
    return 0;
}
